    "version": "0.2.0",
    "configurations": [


        {
            "name": "Python: Current File (internalConsole)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "internalConsole",
            "stopOnEntry": true,
            "cwd": "${workspaceRoot}",
            "env": {"name":"value"}
        },
        {
            "name": "Python: Current File (integratedTerminal)",
            "type": "python",
            "request": "launch",
            "stopOnEntry": true,
            "console": "integratedTerminal",
            "program": "${file}",
            "cwd": "${workspaceRoot}",
            "env": {"name":"value"}
        }
    ]
}